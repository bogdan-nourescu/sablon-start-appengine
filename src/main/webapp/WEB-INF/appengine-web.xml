<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>${app.appid}</application>
    <module>default</module>
    <!--App Config ${app.configSettings} End App COnfig-->
	
    <threadsafe>true</threadsafe>
	
    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
        <property name="appengine.datastore.disableChunkSizeWarning" value="true"/>
		<property name="appengine.api.urlfetch.defaultDeadline" value="30"/>
    </system-properties>
	

</appengine-web-app>
